{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": ["ESNext", "DOM"],
    "outDir": "./build/",
    "sourceMap": true,
    "noImplicitAny": false,
    "module": "ESNext",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "strict": true,
    "types": ["vite/client"]
  }
}
